import java.util.*;

public class Stamp {
    public int getMinimumCost(String desiredColor, int stampCost, int pushCost) {
        int min = stampCost + pushCost * desiredColor.length();
        int cost, i;
        for (int L = 2; L <= desiredColor.length(); L++) {
            i = 0; cost = 0;

            min = Math.min(min, cost);
        }
        return min;
    }
}
